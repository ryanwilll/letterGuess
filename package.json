{
  "name": "letterguess",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "device:android": "npx expo run:android",
    "device:ios": "npx expo run:ios",
    "device:android:no-cache": "npx expo run:android --no-build-cache --device",
    "device:ios:no-cache": "npx expo run:ios --device --no-build-cache",
    "build:android": "eas build --platform android --clear-cache",
    "build:android:local": "eas build --local --platform android --clear-cache",
    "install:dependencies": "npm i && cd ios && pod install && cd ..",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dpcheck": "depcheck"
  },
  "dependencies": {
    "expo": "~53.0.13",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "7.2.2",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
